# Database Fetch Configuration
# Copy this file to .env and fill in your actual credentials

# Environment to fetch from (production or staging)
FETCH_ENVIRONMENT=production

# Production Database Credentials
PROD_DB_HOST=xxx.rds.amazonaws.com
PROD_DB_NAME=xxx
PROD_DB_USER=postgres
PROD_DB_PASSWORD=xxx
PROD_DB_PORT=1357

# Staging Database Credentials
STAGING_DB_HOST=xxx-staging.rds.amazonaws.com
STAGING_DB_NAME=xxx_staging
STAGING_DB_USER=postgres
STAGING_DB_PASSWORD=xxx
STAGING_DB_PORT=1357

# Local Database Credentials (usually these don't need to change)
LOCAL_DB_HOST=localhost
LOCAL_DB_NAME=dbservice_dev
LOCAL_DB_USER=postgres
LOCAL_DB_PASSWORD=postgres
LOCAL_DB_PORT=5432

# Optional: Custom dump file name
DUMP_FILE=dump.sql
